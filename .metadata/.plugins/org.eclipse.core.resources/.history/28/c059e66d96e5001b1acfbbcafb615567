package com.ektha.corejava.employee;

import java.time.LocalDate;

public class TestEmployee {

	public static void main(String[] args) {

		PermanentEmployee employee = new PermanentEmployee();

		employee.setName("Anamika");
		employee.setDob(LocalDate.of(2020, 3, 7));
		employee.setEmail("anamika@gmail.com");
		employee.setPhoneNumber("345267");
		employee.setGender(GenderType.FEMALE);
		employee.setDateOfJoining(LocalDate.of(2020, 10, 12));

		System.out.println(employee.getName());
		System.out.println(employee.getDob());
		System.out.println(employee.getEmail());
		System.out.println(employee.getPhoneNumber());
		System.out.println(employee.getGender());
		System.out.println(employee.getDateOfJoining());
	}

}
